<Window x:Class="Develop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Develop"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="1000" Loaded="Window_Loaded"
        KeyDown="MazeCanvas_KeyDown">
    <Grid Height="1000" KeyDown="MazeCanvas_KeyDown">
        <Canvas x:Name="MazeCanvas" KeyDown="MazeCanvas_KeyDown"/>
    </Grid>
</Window>
